import Header from "../modules/Header/Header.astro";
import Menu from "../modules/Menu/Menu.astro";
import ProductHead from "../modules/ProductHead/ProductHead.astro";
---

<Layout>
  <Header />
  <Menu />

  <Breadcrumbs
    path={[
      "Главная",
      "Каталог",
      "Туристические аксессуары",
      "Набор походный Сплав Track",
